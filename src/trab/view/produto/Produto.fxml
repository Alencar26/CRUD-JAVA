<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="491.0" prefWidth="785.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="trab.view.produto.ControllerProduto">
   <children>
      <TextField fx:id="txtNomeProduto" layoutX="32.0" layoutY="164.0" promptText="Ex: Abobrinha" />
      <TextField fx:id="txtValor" layoutX="32.0" layoutY="233.0" promptText="Ex: R$12,00" />
      <Button fx:id="btnAdicionar" layoutX="33.0" layoutY="362.0" mnemonicParsing="false" onAction="#AdicionarProduto" prefHeight="26.0" prefWidth="149.0" text="Adicionar" />
      <Button fx:id="btnEditar" layoutX="32.0" layoutY="402.0" mnemonicParsing="false" onAction="#EditarProduto" prefHeight="26.0" prefWidth="149.0" text="Editar" />
      <Button fx:id="btnDeletar" layoutX="32.0" layoutY="442.0" mnemonicParsing="false" onAction="#DeletarProduto" prefHeight="26.0" prefWidth="149.0" text="Deletar" />
      <Button fx:id="btnPesquisar" layoutX="475.0" layoutY="117.0" mnemonicParsing="false" onAction="#BuscarProduto" prefHeight="26.0" prefWidth="101.0" text="Pesquisar" />
      <TextField fx:id="txtPesquisar" layoutX="589.0" layoutY="117.0" prefHeight="26.0" prefWidth="160.0" promptText="Pesquisar ID" />
      <TextArea fx:id="txtAreaListaProdutos" layoutX="285.0" layoutY="160.0" prefHeight="308.0" prefWidth="464.0" />
      <Label layoutX="33.0" layoutY="143.0" text="Nome Produto" />
      <Label layoutX="35.0" layoutY="209.0" text="Valor" />
      <Label layoutX="36.0" layoutY="279.0" text="Data Entrada" />
      <Label layoutX="21.0" layoutY="25.0" text="PRODUTOS" textFill="#f50a0a">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <DatePicker fx:id="txtData" layoutX="28.0" layoutY="301.0" prefHeight="26.0" prefWidth="160.0" promptText="12/12/2012" />
   </children>
</AnchorPane>
